@model IEnumerable<EnroleClass.Models.Class>

@{
    ViewBag.Title = "StudentClasses";
}

<h2>Student Classes</h2>

<table class="table">
    <tr>
        <th>
            Name
        </th>
        <th>
            Description
        </th>
        <th>
            Price
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClassName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClassDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClassPrice)
        </td>
      
    </tr>
}

</table>
